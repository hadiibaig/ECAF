<script>
    $(document).ready(function() {
        $('.formRow').click(function () {
            var id = $(this).data('id');
            $.ajax({
                url: '@Url.Action("ViewForm", "Form")',
                type: 'GET',
                data: { id: id },
                success: function(result) {
                    $('#content').empty();
                    $('#content').html(result);
                },
                error: function(xhr, status, error) {
                    console.log("Error loading child partial view: " + error);
                }
            });
        });
    });
    function showTab(tabId) {
        document.querySelectorAll('.tab-content').forEach(tab => {
            tab.classList.remove('active');
        });
        document.getElementById(tabId).classList.add('active');
        document.querySelectorAll('.formBookList').forEach(tabBtn => {
            tabBtn.classList.remove('active');
        });
        document.getElementById('tabBtn' + tabId.replace('tab', '')).classList.add('active');
    }
    document.addEventListener('DOMContentLoaded', () => {
        showTab('tab1');
    });
</script>