        $('.mainMenuCloseIcon').click(function () {
            $(this).closest('.mainMenuDropBox').removeClass('active');
        });
        $('#SetUpSiteCard').click(function () {
            window.location.href = '/SiteCard/SetUpSiteCard';
        });
        $('#UpdateSiteCard').click(function () {
            
            $('#attached_adhocPop').show()
        });
        $('#attached_adhocCont').click(function () {
              window.location.href = '/SiteCard/UpdateSiteCard';
        });
        $('#TerminatingSiteCard').click(function () {
            window.location.href = '/SiteCard/TerminatingSiteCard';
        });
        $('#ECAF_Form').click(function () {
