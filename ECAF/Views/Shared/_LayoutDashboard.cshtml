    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/Scripts/jquery.signalR-2.4.3.min.js"></script>
    <script src="~/signalr/hubs"></script>
    <script src="~/Scripts/custom/notifications.js"></script>
    <script>
        $(document).ready(function () {
            // Main Menu
            $('#mainMenuDrop').click(function () {
                $(this).toggleClass('active');
                $('.mainMenuDropBox').toggleClass('active');
            });
            $('.mainMenuCloseIcon').click(function () {
                $(this).closest('.mainMenuDropBox').removeClass('active');
            });
            $('a').click(function (e) {
                e.preventDefault();
                var url = $(this).data('url');
                if (url != null && url != undefined) {
                    if (url == "/Calender") {
                        window.location.href = '/Calender'
                    }
                    if (url == "/") {
                        window.location.href = '/Home'
                    }
                    else {
                        $('#content').load(url);
                    }
                }

            });

            $('.sectionRow').click(function () {
                var id = $(this).data('id');
                var data = null;
                if ($(this).data('title') == 'comments') {
                    data = { id: id, section: 'comments' }
                }
                else {
                    data = { id: id }
                }
            $.ajax({
                url: '@Url.Action("ViewForm", "Form")',
                type: 'GET',
                data: data,
                success: function(result) {
                    $('#content').empty();
                    $('#content').html(result);
                },
                error: function(xhr, status, error) {
                    console.log("Error loading child partial view: " + error);
                }
            });
        });
        });
        function showlist(tabId) {
            document.getElementById(tabId).classList.add('active');
            document.querySelectorAll('.formBookLists').forEach(tabBtn => {
                if (tabBtn.id != tabId) {
                    tabBtn.classList.remove('active');
                }
            });
        }
        document.addEventListener('DOMContentLoaded', () => {
            showlist('li1');
        });
    </script>
